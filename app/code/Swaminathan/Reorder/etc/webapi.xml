<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/swaminathan/reorder/:orderId" method="POST">
        <service class="Swaminathan\Reorder\Api\ReorderInterface" method="reorderItem"/>
        <resources>
            <resource ref="self" />
        </resources>
    </route>
</routes>