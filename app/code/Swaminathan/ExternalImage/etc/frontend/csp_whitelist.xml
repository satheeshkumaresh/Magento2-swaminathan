<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="form-action">
            <values>
                <value id="swaminathan-loc-https" type="host">*.swaminathan.com</value> 
            </values>
        </policy>   
        <policy id="img-src">
            <values>
                <value id="swaminathan-loc-img-https" type="host">http://swaminathan.com</value> 
                <value id="swaminathan-loc-img-live" type="host">https://swaminathan.com</value> 
            </values>
        </policy>          
    </policies>
</csp_whitelist>
